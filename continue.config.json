{
  "agent": {
    "enabled": true,
    "allowTerminal": true,
    "allowFileAccess": true,
    "allowCodeEdits": true,
    "terminalRestrictions": ["rm -rf", "shutdown", "reboot", "mkfs", "dd", ":(){:|:&};:"],
    "fileAccessScope": ["/Users/fahmadiqbal/FHIR-DICOM-With-Med-Gemma"],
    "troubleshooting": {
      "workspaceTrustRequired": true,
      "extensionVersionCheck": true,
      "showAgentErrors": true
    },
    "logging": true,
    "logFile": "agent-actions.log"
  },
  "models": [
    {
      "provider": "ollama",
      "model": "deepseek-coder-v2-lite",
      "title": "DeepSeek Coder"
    },
    {
      "provider": "ollama",
      "model": "qwen2.5-coder:7b",
      "title": "Qwen2 Coder"
    },
    {
      "provider": "ollama",
      "model": "starcoder2:15b",
      "title": "StarCoder2"
    }
  ],
  "completionOptions": {
    "temperature": 0.2,
    "maxTokens": 2000,
    "top_p": 0.95,
    "presence_penalty": 0.1
  }
}
